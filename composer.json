{
  "name": "bridit/console",
  "description": "Brid Modular Micro-Framework (Http)",
  "license": "MIT",
  "authors": [
    {
      "name": "Gustavo Siqueira",
      "email": "gus@brid-it.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "bridit/core": "^1.0",
    "symfony/console": "^5.3",
    "symfony/process": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.88"
  },
  "autoload": {
    "psr-4": {
      "Brid\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
